<%= simple_form_for(@project, url: projects_path, :method => :post) do |f| %>
  <%= f.input :title %>
  <%= f.input :description, as: :rich_text_area %>
  <%= f.input :skills, as: :select,
      collection: Skill.pluck(:title, :id) %>
  <%= f.input :date_start, as: :date %>
  <%= f.input :date_end, as: :date %>
  <%= f.input :finished %>
  <%= f.input :url %>
  <%= f.input :collaborators_desc %>
  <%= f.input :photo, as: :file %>
  <%= f.submit "Enregistrer", class: "btn btn-primary" %>
<% end %>
